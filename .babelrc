{
  "presets": [
    ["@babel/preset-react"],
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "targets": {
          "browsers": ["last 2 versions", "safari >= 7"]
        }
      }
    ],
    ["@babel/preset-typescript"]
  ],
  "plugins": ["@babel/plugin-syntax-dynamic-import"]
}
